[ovirt-{{ ovirt.version }}]
name=Latest oVirt {{ ovirt.version }} Release
#baseurl=http://resources.ovirt.org/pub/ovirt-{{ ovirt.version }}/rpm/el$releasever/
mirrorlist=http://resources.ovirt.org/pub/yum-repo/mirrorlist-ovirt-{{ ovirt.version }}-el$releasever
{%  if 'ovirt-{{ ovirt.version }}' in package_repo.enabled_repos %}
enabled=1
{% else %}
enabled=0
{% endif %}
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-ovirt-{{ ovirt.version }}
